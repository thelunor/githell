<!DOCTYPE html>
<html>
	<meta charset="UTF-8">
<style>
table,th,td {
  border : 1px solid black;
  border-collapse: collapse;
}
th,td {
  padding: 5px;
}
</style>
<body>

<button type="button" onclick="loadXMLDoc()">Get my CD collection</button>
<br><br>
<table id="demo"></table>

<script>
function loadXMLDoc() {
  var xmlhttp = new XMLHttpRequest();
  xmlhttp.onreadystatechange = function() {
    if (this.readyState == 4 && this.status == 200) {
      myFunction(this);
    }
  };
  xmlhttp.open("POST", "https://openapi.gg.go.kr/RegionMnyFacltStus?Key=8c5ccadd5f304ba78220ca66e6f4885e&Type=xml&pIndex=1&pSize=100&SIGUN_CD=41190", true);
  xmlhttp.send();
}
function myFunction(xml) {
  var i;
  var xmlDoc = xml.responseXML;
  var table="<tr><th>가맹점</th><th>업종코드</th><th>지역화폐</th><th>도로명 주소</th><th>시군코드</th></tr>";
  var x = xmlDoc.getElementsByTagName("row");
  for (i = 0; i <x.length; i++) { 
    table += "<tr><td>" +
    x[i].getElementsByTagName("CMPNM_NM")[0].childNodes[0].nodeValue + "</tr></td>" +
    "<tr><td>" +
    x[i].getElementsByTagName("INDUTYPE_CD")[0].childNodes[0].nodeValue + "</td></tr>" +
    "<tr><td>" +
    x[i].getElementsByTagName("REGION_MNY_NM")[0].childNodes[0].nodeValue + "</td></tr>" +
    "<tr><td>" +
    x[i].getElementsByTagName("REFINE_LOTNO_ADDR")[0].childNodes[0].nodeValue + "</td></tr>" +
    "<tr><td>" +
    x[i].getElementsByTagName("SIGUN_CD")[0].childNodes[0].nodeValue + "</td></tr>";
  }
  document.getElementById("demo").innerHTML = table;
}
</script>

</body>
</html>